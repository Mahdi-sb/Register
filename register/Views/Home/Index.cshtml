@{
    ViewData["Title"] = "Home Page";
}

@if (TempData["Name"] == null)
{
    <div class="text-center">
        <h1 class="display-4">Welcome</h1>
        <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core @TempData["hi"]</a>.</p>
    </div>
}
else
{
    <div class=" card-body">
        <h1 class="display-4">Welcome @TempData["UserName"]</h1>
        <div class="card-footer">
             Email Address: @TempData["Email"]
            <br />
            Name: @TempData["Name"]
        </div>
        
    </div>
}
